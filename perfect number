public class basics{
    public static void main(String[] args) {
        int i,sum=0,n=6;
        for(i=1;i<n;i++){
            if(n%i==0){
                sum=sum+i;
            }
        }
        if(sum==n){
            System.out.println("perfect number");
        }else{
            System.out.println("not");
        }
    }}
