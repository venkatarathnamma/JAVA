public class basics{
    public static void main(String[] args) {
        int i,r,reverse=0,n=121;
        int original=n;
        while(n>0){
            r=n%10;
            reverse=reverse*10+r;
            n=n/10;
        }
        if(original==reverse){
            System.out.println("palindrome");
        }else{
            System.out.println("not palindrome");
        }
    }}
