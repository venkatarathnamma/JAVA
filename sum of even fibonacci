public class basics{
    public static void main(String[]args){
        int a=0,b=1,n=10,evensum=0,sum=0,i;
        for(i=2;i<=n;i++){
            System.out.println(a+ " ");
            if(a%2==0){
                evensum+=a;
            }
            sum=a+b;
            a=b;
            b=sum;
        }
            System.out.println("even sum is" +evensum);
        }
        
    }
