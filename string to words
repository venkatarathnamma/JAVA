import java.util.Scanner;
public class split{
    public static void main(String[]args){
        Scanner sc=new Scanner(System.in);
        System.out.print("enter string: ");
        String s=sc.nextLine();
        String[] words=s.split(" ");
        for(String w:words){
            System.out.println("splitting of words: " +w);
        }
    }
}
