public class basics{
    public static void main(String[] args) {
        int i,n,start=5,end=30;
        for(n=start;n<=end;n++){
            int sum=0;
            for(i=1;i<n;i++){
                if(n%i==0){
                    sum=sum+i;
                }
            }
            if(n==sum){
                System.out.println(n);
            }
        }
    }}
