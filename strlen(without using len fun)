import java.util.Scanner;
public class string{
    public static void main(String[]args){
        Scanner sc=new Scanner(System.in);
        System.out.print("enter string: ");
        String s=sc.nextLine();
        int count=0;
        char c[]=s.toCharArray();
        for(char ch:c){
            count++;
        }
        System.out.print(count);
    }
}
